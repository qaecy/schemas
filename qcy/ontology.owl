        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The file path property specifies the location of a file on a storage system. This is always a relative path in relation to the project directory on a cloud storage provider.</rdfs:comment>
        <rdfs:label xml:lang="en">file path</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://qaecy.com/dev/ont#label -->


    <owl:DatatypeProperty rdf:about="https://qaecy.com/dev/ont#label">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <schema:domainIncludes rdf:resource="https://qaecy.com/dev/ont#CanonicalEntity"/>
        <schema:domainIncludes rdf:resource="https://qaecy.com/dev/ont#EntityMention"/>
        <rdfs:comment xml:lang="en">A label is a human-readable name for the resource. Each resource should have only one preferred label in english that specifies the resource.</rdfs:comment>
        <rdfs:label xml:lang="en">label</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://qaecy.com/dev/ont#tag -->


    <owl:DatatypeProperty rdf:about="https://qaecy.com/dev/ont#tag">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <schema:domainIncludes rdf:resource="https://qaecy.com/dev/ont#InformationObject"/>
        <rdfs:comment xml:lang="en">A tag is a keyword or term assigned to a piece of information, such as a qcy:InformationObject, to describe and categorize it. Tags are typically used to facilitate searching and organizing information. An information object can have multiple tags.</rdfs:comment>
        <rdfs:label xml:lang="en">tag</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://qaecy.com/dev/ont#textSummary -->


    <owl:DatatypeProperty rdf:about="https://qaecy.com/dev/ont#textSummary">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <schema:domainIncludes rdf:resource="https://qaecy.com/dev/ont#InformationObject"/>
        <rdfs:comment xml:lang="en">A short text that summarizes all information about the thing. This is typically generated by a Large Language model from provided context any form.</rdfs:comment>
        <rdfs:label xml:lang="en">text summary</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://qaecy.com/dev/ont#value -->


    <owl:DatatypeProperty rdf:about="https://qaecy.com/dev/ont#value">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <schema:domainIncludes rdf:resource="https://qaecy.com/dev/ont#Selector"/>
        <rdfs:comment xml:lang="en">A value is a text string that holds the value. How to interpret this value depends on what the value is assigned to.</rdfs:comment>
        <rdfs:label xml:lang="en">value</rdfs:label>
    </owl:DatatypeProperty>
    


