    <!-- https://qaecy.com/dev/ont#about -->


    <owl:ObjectProperty rdf:about="https://qaecy.com/dev/ont#about">
        <rdfs:domain rdf:resource="https://qaecy.com/dev/ont#InformationObject"/>
        <rdfs:range rdf:resource="https://qaecy.com/dev/ont#CanonicalEntity"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://qaecy.com/dev/ont#mentions"/>
            <rdf:Description rdf:about="https://qaecy.com/dev/ont#resolvesTo"/>
        </owl:propertyChainAxiom>
        <rdfs:comment>Relationship between a qcy:InformationObject and a qcy:CanonicalEntity. This property indicates that a file content or fragment is about a specific canonical entity. It can be implicitly derived that if a file content or fragment mentions the occurrence of a named entity, and that named entity resolves to a canonical entity, then the file content or fragment is about the canonical entity.</rdfs:comment>
        <rdfs:label xml:lang="en">about</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://qaecy.com/dev/ont#containsFragment -->


    <owl:ObjectProperty rdf:about="https://qaecy.com/dev/ont#containsFragment">
        <rdfs:domain rdf:resource="https://qaecy.com/dev/ont#InformationObject"/>
        <rdfs:range rdf:resource="https://qaecy.com/dev/ont#Fragment"/>
        <rdfs:comment>Relationship between a qcy:InformationObject and one of its qcy:Fragments</rdfs:comment>
        <rdfs:label xml:lang="en">contains fragment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://qaecy.com/dev/ont#hasFileLocation -->


    <owl:ObjectProperty rdf:about="https://qaecy.com/dev/ont#hasFileLocation">
        <rdfs:domain rdf:resource="https://qaecy.com/dev/ont#FileContent"/>
        <rdfs:range rdf:resource="https://qaecy.com/dev/ont#FileLocation"/>
        <rdfs:comment>Relationship between a qcy:FileContent object and one of its qcy:FileLocations</rdfs:comment>
        <rdfs:label xml:lang="en">has file location</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://qaecy.com/dev/ont#hasProperty -->


    <owl:ObjectProperty rdf:about="https://qaecy.com/dev/ont#hasProperty">
        <rdfs:domain rdf:resource="https://qaecy.com/dev/ont#ProjectEntity"/>
        <rdfs:range rdf:resource="https://qaecy.com/dev/ont#Property"/>
        <rdfs:comment>Relationship between a qcy:EntityMention or qcy:CanonicalEntity and one of its qcy:Properties.</rdfs:comment>
        <rdfs:label xml:lang="en">has property</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://qaecy.com/dev/ont#mentions -->


    <owl:ObjectProperty rdf:about="https://qaecy.com/dev/ont#mentions">
        <rdfs:domain rdf:resource="https://qaecy.com/dev/ont#InformationObject"/>
        <rdfs:range rdf:resource="https://qaecy.com/dev/ont#EntityMention"/>
        <rdfs:comment>Relationship between a qcy:InformationObject and a qcy:EntityMention. This property indicates that a file content or fragment mentions a specific named entity. This relationship can implicitly be derived from the subject/object pair of a qcy:Selector.</rdfs:comment>
        <rdfs:label xml:lang="en">mentions</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://qaecy.com/dev/ont#relatedEntity -->


    <owl:ObjectProperty rdf:about="https://qaecy.com/dev/ont#relatedEntity">
        <rdfs:domain rdf:resource="https://qaecy.com/dev/ont#ProjectEntity"/>
        <rdfs:range rdf:resource="https://qaecy.com/dev/ont#ProjectEntity"/>
        <rdfs:comment>Relationship between either a pair of qcy:EntityMention or qcy:CanonicalEntity occurrences (never between a qcy:EntityMention and a qcy:CanonicalEntity!). This is a general property that should not be used explicitly. Instead, use one of the more specific sub-properties.</rdfs:comment>
        <rdfs:label xml:lang="en">related entity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://qaecy.com/dev/ont#resolvesTo -->


    <owl:ObjectProperty rdf:about="https://qaecy.com/dev/ont#resolvesTo">
        <rdfs:domain rdf:resource="https://qaecy.com/dev/ont#EntityMention"/>
        <rdfs:range rdf:resource="https://qaecy.com/dev/ont#CanonicalEntity"/>
        <rdfs:comment>Relationship between a qcy:EntityMention and a qcy:CanonicalEntity. This property indicates that a named entity occurrence resolves to a canonical entity, meaning that the named entity is recognized as an instance of the canonical entity.</rdfs:comment>
        <rdfs:label xml:lang="en">resolves to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://qaecy.com/dev/ont#selectorObject -->


    <owl:ObjectProperty rdf:about="https://qaecy.com/dev/ont#selectorObject">
        <rdfs:domain rdf:resource="https://qaecy.com/dev/ont#Selector"/>
        <rdfs:range rdf:resource="https://qaecy.com/dev/ont#EntityMention"/>
        <rdfs:comment>Relationship between a qcy:Selector object and the qcy:EntityMention it specifies.</rdfs:comment>
        <rdfs:label xml:lang="en">selector object</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://qaecy.com/dev/ont#selectorSubject -->


    <owl:ObjectProperty rdf:about="https://qaecy.com/dev/ont#selectorSubject">
        <rdfs:domain rdf:resource="https://qaecy.com/dev/ont#Selector"/>
        <rdfs:range rdf:resource="https://qaecy.com/dev/ont#InformationObject"/>
        <rdfs:comment>Relationship between a qcy:Selector object and the qcy:InformationObject it specifies.</rdfs:comment>
        <rdfs:label xml:lang="en">selector subject</rdfs:label>
    </owl:ObjectProperty>
    


