    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    <rdf:Description rdf:about="https://qaecy.com/dev/ont#NamedEntity">
        <rdfs:comment xml:lang="en">A named entity is an occurrence of a specific entity within a qcy:FileContent object. Each occurrence is unique, meaning that two mentions of &quot;Albert Einstein&quot; in the same fragment are considered two distinct named entities. Named entities can include mentionings of persons, organizations, locations, dates, and other proper nouns.</rdfs:comment>
        <rdfs:label xml:lang="en">Named Entity</rdfs:label>
    </rdf:Description>
</rdf:RDF>



