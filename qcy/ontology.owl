        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">A boolean property specifying that something is explicit. It is specifically used to indicate that a qcy:CanonicalEntity has been confirmed by an authorized project practitioner or other specialist.</rdfs:comment>
        <rdfs:label xml:lang="en">explicit</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://qaecy.com/dev/ont#filePath -->


    <owl:DatatypeProperty rdf:about="https://qaecy.com/dev/ont#filePath">
