    <!-- https://qaecy.com/dev/ont#explicit -->


    <owl:DatatypeProperty rdf:about="https://qaecy.com/dev/ont#explicit">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <schema:domainIncludes rdf:resource="https://qaecy.com/dev/ont#CanonicalEntity"/>
        <rdfs:comment xml:lang="en">A boolean property indicating whether something is explicit. It is specifically used to indicate that a qcy:CanonicalEntity has been confirmed by an authorized project practitioner or other specialist.</rdfs:comment>
        <rdfs:label xml:lang="en">explicit</rdfs:label>
    </owl:DatatypeProperty>
    


